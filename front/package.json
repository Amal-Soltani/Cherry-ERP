    "start-ar": "node git.version.js && ng serve --port=4204 --proxy-config proxy.conf.json --configuration ar",
    "build": "node git.version.js prod && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build",
    "test": "ng test",
    "lint": "ng lint",
    "modernizr": "modernizr -c modernizr-config.json -d src/modernizr.js",
    "webdriver-manager-update": "node ./node_modules/protractor/bin/webdriver-manager update",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --port=4202 --proxy-config proxy.conf.json --configuration fr",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/fr/stats.json"
